{
  "expo": {
    "name": "IBT",
    "slug": "IBT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ibt SC.png",
    "scheme": "ibtmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/ibt SC.png",
      "resizeMode": "contain",
      "backgroundColor": "#053827"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#053827" 
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.IBT"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ibt SC.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#053827" 
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }

}
